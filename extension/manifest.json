{
  "description": "Demonstrating webRequests",
  "manifest_version": 2,
  "name": "Streamie",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "<all_urls>"
  ],

  "browser_action": {
    "default_icon": "icons/copy.svg",
    "default_title": "Streamie",
    "default_popup": "popup/popup.html"
  },

  "background": {
    "scripts": ["scripts/background.js"]
  }
}